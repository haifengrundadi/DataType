#include "leetcode.h"
#include<stdint.h>

/**<<超时了>*/
class Solution {
public:
	bool containsNearbyDuplicate(vector<int>& nums, int k) {
		bool flag = false;
		if(k <= 0 || nums.size() <= 1) return false;
		int j = 0;
		for(int i = 0; i < nums.size(); i++){
			if(i + k >= nums.size()-1){
				j = nums.size() - 1;
			}else{
				j = i + k;
			}
			cout<<"i--"<<i<<"--j--"<<j<<endl;
			while(j > i){
					if(nums[i] == nums[j]){
							return true;
					}
					j--;
				}
			}
		return flag;
	 }
};

int main(){
	Solution * s= new Solution();
	int nums[] = {1,2,3,4,5,6,1};
    vector<int> ass;
    int i = 0;
    while(i < 7){
    	ass.push_back(nums[i]);
    	i++;
    }
	cout<<s->containsNearbyDuplicate(ass,5);
	return 0;
}
