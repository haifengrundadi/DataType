#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
		int trailingZeroes(int n) {
			int count = 0;
			if(n == 0) return count;
			int len = log10(n) + 1;
			if(n < 5) return 0;
			else if(len == 1 && n >=5) return 1;
			while(--len){
//				cout<<"n = "<<n<<"   len ="<<len<<endl;
				count += (pow(10, len -1) * 2  + (len - 1)) *  (n / pow(10, len));
				n -= pow(10, len) * (n / pow(10, len));
			}
			return count;
	    }
};

int main(){
	Solution * s= new Solution();
	for(int i = 1; i<= 1001; i++){
		cout<<"\t"<<i<<"---"<<s->trailingZeroes(i)<<endl;
		cout<<"***************"<<endl;
	}
	return 0;
}
