#include "LinkedList.h"
using namespace std;

template<class T>
List<T>::List(List<T>& L)
{
	//复制构造函数
	T value;
	LinkNode<T>* srcptr = L.getHead();
	LinkNode<T>* destptr = new LinkNode<T>;
	while(srcptr->link != NULL)
	{
		value = srcptr->link->data;
		destptr->link = new LinkNode<T>(value);
		destptr= destptr->link;
		srcptr = srcptr->link;
	}
}
