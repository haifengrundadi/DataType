#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
		int factor(int n){
			int total = 1;
			for(int i = 1 ; i <= n; i++){
				total *= i;
			}
			return total;
		}
		void trailingZeroes(int n) {
			for(int i = 1; i < n; i++){
				cout<<i<<"---"<<factor(i)<<endl;
			}
	    }
};

int main(){
	Solution * s= new Solution();
	s->trailingZeroes(20);
	return 0;
}
