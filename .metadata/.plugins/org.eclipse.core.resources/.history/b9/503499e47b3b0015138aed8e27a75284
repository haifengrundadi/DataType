		int len = ceil(log10(x));
		cout<<len<<endl;
		if(abs(x) < 10)
		{
			return x;
		}else{
			if(x <= -10)
			{

			}
		}
	}
};

int main()
{
	Solution * s= new Solution();
	cout<<s->reverse(11)<<endl;
	return 0;
}
