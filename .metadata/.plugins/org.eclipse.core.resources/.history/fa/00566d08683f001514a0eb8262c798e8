#include<stdlib.h>
#include "leetcode.h"


class Solution {
public:
	 int compareVersion(string version1, string version2) {
		 	int indexA = version1.find_first_not_of('.',0);
		 	int indexB= version2.find_first_not_of('.',0);
			string strA( version1.substr(0,indexA));
			string strAA( version1.substr(indexA));
			string strB= version2.substr(0,indexB);
			string strBB= version2.substr(indexB);
			int strA_i = atoi(strA.c_str());
			int strB_i = atoi(strB.c_str());
			if(strA_i > strB_i) return 1;
			if(strA_i < strB_i) return -1;
			if(strA_i == strB_i){
				int i = 0;
				int len_a =strAA.length();
				int len_b = strBB.length();
				while(i < len_a && i < len_b){
					if(strAA[i] > strBB[i]) return 1;
					if(strAA[i] < strBB[i]) return -1;
					i++;
				}
				if(i < len_a){
					return 1;
				}
				if(i < len_b)
					return -1;
			}
			return 0;
	}
};
int main(){
	Solution * s= new Solution();
	return 0;
}
