#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
	int titleToNumber(string s) {
		int len = s.length();
		if(len <= 0) return 0;
		map<char,int> m;
		int col = 0;
		cout<<len<<endl;
		for(int i = 0; i < 26; i++){
			m.insert(pair<char,int>(('A'+i),i+1));
		}
		cout<<endl;
		while(len){
			cout<<s[len-1]<<"----"<<m.find(s[len - 1])->second <<"----"<< pow(26, len - 1)<<"---*";
			col += m.find(s[len - 1])->second * pow(26, len - 1);
			len--;
		}
		cout<<endl;
		return col;
	}
};

int main(){
	Solution * s= new Solution();
	string str = "A";
	cout<<s->titleToNumber(str)<<endl;
	 str = "AB";
	cout<<s->titleToNumber(str)<<endl;
	 str = "ABC";
	cout<<s->titleToNumber(str)<<endl;
	 str = "AAA";
	cout<<s->titleToNumber(str)<<endl;
	return 0;
}
