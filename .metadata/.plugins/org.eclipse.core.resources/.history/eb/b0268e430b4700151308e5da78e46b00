#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
	bool isUgly(int num) {
		if(num <= 0) return 0;
		if(num == 1) return 1;
		for (int i = 2; i < num; i++) {
			if( num % i == 0 && i > 5){
				if(!isUgly(i))
					return false;
				else if(num % i ==0 &&( i == 2 || i== 3 || i==5)){
					continue;
				}
			}
		}
		return true;
	}
};

int main(){
	Solution * s= new Solution();
	cout<<s->isUgly(6)<<endl;
	cout<<s->isUgly(14)<<endl;
	cout<<s->isUgly(8)<<endl;
	cout<<s->isUgly(2)<<endl;
	cout<<s->isUgly(3)<<endl;
	cout<<s->isUgly(7)<<endl;
	return 0;
}
