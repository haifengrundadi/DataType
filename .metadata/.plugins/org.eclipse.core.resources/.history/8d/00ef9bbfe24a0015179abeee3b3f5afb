#include "leetcode.h"

class Solution {
public:
	vector<int> grayCode(int n) {
		vector<int> v;
		for(int i =1; i < n; i++){
			v.push_back(0);
			v.push_back(1);
		}
		return v;
	 }
};

int main()
{
	Solution * s = new Solution();
	vector<int> nums;
	nums = s->grayCode(1);
	vector<int>::iterator it;
	for(it = nums.begin(); it != nums.end();it++){
		cout<<*it<<"\t";
	}
	return 0;
}
