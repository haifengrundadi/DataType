#ifndef HEADER_LINKNODE_H_
#define HEADER_LINKNODE_H_
#include <iostream>
template <class T>
struct LinkNode {
	T data;
	LinkNode<T>* link; //链指针域
	LinkNode(LinkNode<T>* ptr = NULL) {link = ptr;} //仅初始化指针成员的构造函数
	LinkNode(const T& item, LinkNode<T>* ptr = NULL)
	{
		data = item;
		link = ptr;
	}
};

#endif /* HEADER_LINKNODE_H_ */
