#include "leetcode.h"

class Solution {
public:
	bool isValid(string s) {
		bool flag = false;
		vector<char> v;
		for(int i =0; i < s.length(); i++){
			if(s[i] == ')' && ! v.empty()){
				if(v.back() != '(')
					return false;
				else
					v.pop_back();
			}else if(s[i] == '}' && ! v.empty()){
					if(v.back() != '{')
						return false;
					else
						v.pop_back();
			}else if(s[i] == ']' && ! v.empty()){
				if(v.back() != '[')
					return false;
				else
					v.pop_back();
			}else{
				v.push_back(s[i]);
			}
		}
		return flag;
	}
};

int main()
{
	Solution * s= new Solution();
	return 0;
}
