#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
		int trailingZeroes(int n) {
			int count = 0,j = 0;
			for(int i = 1; i <= n; i++){
				if(i % 10 == 0){
					j = i;
					while(j % 10 == 0){
						count = count + 1;
						j = j /10;
					}
				}else if(i % 5 == 0){
					count++;
				}
			}
			return count;
	    }
};

int main(){
	Solution * s= new Solution();
	for(int i = 1; i<= 20; i++){
		cout<<s->trailingZeroes(i)<<endl;
	}
	return 0;
}
