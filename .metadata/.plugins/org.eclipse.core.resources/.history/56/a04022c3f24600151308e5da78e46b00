#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
	bool isUgly(int num) {
		if(num <= 0) return 0;
		if(num == 1) return 1;
		bool arr[num];
		for (int i = 2; i < num; i++)
				arr[i] = true;
		for (int i = 2; i*i < num; i++) {
			if(!arr[i]) continue;
			for(int j = i * i; j < num; j += i){
				arr[j] = false;
			}
		}
		for (int i = 2; i*i < num; i++) {
			if(arr[i]) cout<<i<<"\t"<<arr[i];
		}
		return true;
	}
};

int main(){
	Solution * s= new Solution();
	for(int i = 0;i < 100; i++)
		cout<<"i="<<i<<" isugly="<<s->isUgly(i)<<endl;
	return 0;
}
