#include<iostream>
#include<string>
#include<cmath>
#include<fstream>
#include <stdlib.h>
using namespace std;

class Solution {
public:
	int myAtoi(string str) {
		int i = 0,flag = 1;
		int len = str.length();
		while(i < len && str[i] == ' ')
			i++;
		if(str[i] == '-') flag  = -1;
		else if(str[i]== '+')flag = 1;
		else if(isdigit(str[i])){
			while(){

			}
		}
    }
};
