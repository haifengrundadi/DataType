#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
		int trailingZeroes(int n) {
			int count = 0;
			if(n == 0) return count;
			count += n / 5;
			int total = 0
			while(n > 5){
				count += 1;
				n = n / 5;
			}
			return count;
	    }
};

int main(){
	Solution * s= new Solution();
	for(int i = 1; i<= 1001; i++){
		cout<<"\t"<<i<<"---"<<s->trailingZeroes(i)<<endl;
		cout<<"***************"<<endl;
	}
	return 0;
}
