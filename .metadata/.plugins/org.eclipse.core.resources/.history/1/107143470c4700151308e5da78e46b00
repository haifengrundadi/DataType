#include "leetcode.h"
#include<stdint.h>

class Solution {
public:
	bool isUgly(int num) {
		if(num <= 0) return 0;
		if(num == 1) return 1;
		int k = 0;
		for (int i = 2; i <= num; i++) {
			if( num % i == 0 && i > 5){
				k = i;
			   while(k % 2 == 0 || k % 3 ==0 ||k %5==0){
				   	 k = k%2==0?(k / 2):k;
				   	 k = k%3==0?(k / 3):k;
				   	 k = k%5==0?(k / 5):k;
				}
			   if(k != 1){
				   return false;
			   }
			}
		}
		return true;
	}
};

int main(){
	Solution * s= new Solution();
	cout<<s->isUgly(6)<<endl;
	cout<<s->isUgly(14)<<endl;
	cout<<s->isUgly(8)<<endl;
	cout<<s->isUgly(2)<<endl;
	cout<<s->isUgly(3)<<endl;
	cout<<s->isUgly(7)<<endl;
	return 0;
}
